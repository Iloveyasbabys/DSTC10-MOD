        {
            "speaker_id": "[speaker1]",
            "txt": "\u5f88\u5c0f\u5fc3\u4e86\uff0c\u9a91\u7684\u597d\u6162\u4e86\uff01\uff01"
        },
        {
            "speaker_id": "[speaker2]",
            "txt": "\u597d\u5427\uff0c\u96e8\u5929\uff0c\u6211\u90fd\u4e0d\u9a91\u8f66"
