        {
            "speaker_id": "[speaker1]",
            "txt": "\u91cd\u70b9\u3002\u4e0d\u9a91\u8f66\u8d70\u8def\u597d\u8fdc\u554a\uff01\uff01\uff01"
        },
        {
            "speaker_id": "[speaker2]",
            "txt": "\u6253\u8f66\u2026\u2026\u6211\u90fd\u662f\u4e0a\u73ed\u6253\u8f66\uff0c\u4e0b\u73ed\u8d70\u8def"
        },
        {
            "speaker_id": "[speaker1]",
            "txt": "\u90a3\u6211\u4e00\u5929\u8981\u82b130"
        },
        {
            "speaker_id": "[speaker2]",
            "txt": "\u2026\u2026\u90a3\u4e48\u8fdc\u4e48\uff1f"
        },
        {
            "speaker_id": "[speaker1]",
            "txt": "\u6765\u56de\uff01"
        },
        {
            "speaker_id": "[speaker2]",
            "txt": "\u597d\u5427\uff0c\u6211\u662f\u5728\u8d77\u6b65\u4ef7\u4e4b\u5185"
        },
        {
            "speaker_id": "[speaker1]",
            "txt": "\u600e\u4e48\u4e0d\u51fa\u53bb\u7ea6\u4f1a\u554a\uff01"
