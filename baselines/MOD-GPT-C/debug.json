        {
            "speaker_id": "[speaker2]",
            "emotion_id": 7,
            "img_id": "121",
            "txt": "\u7eb3\u5c3c\uff1f\uff01\u4ec0\u4e48\u60c5\u51b5\uff1f\uff1f\uff1f"
        },
        {
            "speaker_id": "[speaker1]",
            "txt": "\u5730\u6ed1\uff0c\u5239\u8f66\u5239\u4e0d\u4f4f\u4e86\uff0c\u5c31\u6ed1\u8fc7\u53bb\u4e86\uff0c"
        },
        {
            "speaker_id": "[speaker2]",
            "txt": "\u5929\u54ea\uff0c\u4f60\u5c0f\u5fc3\u70b9\u5566\uff01\uff01\uff01"
